<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- CustomButton AttributeSet -->
    <declare-styleable name="CustomButton">
        <attr name="btnAttr_txt" format="string" />
        <attr name="btnAttr_img" format="reference|integer" />
    </declare-styleable>

    <!-- CustomEditText AttributeSet -->
    <declare-styleable name="CustomEditText">
        <attr name="editAttr_txt" format="string" />
        <attr name="editAttr_edit_hint" format="string" />
        <attr name="editAttr_edit_drawable" format="reference|integer" />
        <attr name="editAttr_edit_inputType" format="integer|reference" />
    </declare-styleable>

    <!-- CustomTitle AttributeSet -->
    <declare-styleable name="CustomTitle">
        <attr name="titleAttr_txt" format="string" />
        <attr name="titleAttr_img" format="reference|integer" />
    </declare-styleable>
</resources>
