<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- 상단부 메뉴란 Custom View -->
    <declare-styleable name="TitleAttrs">
        <attr name="title_text" format="string" />
        <attr name="title_symbol" format="reference|integer"/>
        <attr name="title_textSize" format="reference|integer"/>
    </declare-styleable>

    <!-- 중단부 사용자 세부사항 기입란 Custom View -->
    <declare-styleable name="EditAttrs">
        <attr name="edit_text" format="string" />
        <attr name="edit_hint" format="string" />
        <attr name="edit_inputType" format="reference|integer"/>
    </declare-styleable>

    <!-- 하단부 Button Custom View -->
    <declare-styleable name="BtnAttrs">
        <attr name="btn_left_text" format="string" />
        <attr name="btn_right_text" format="string" />
        <attr name="btn_left_img" format="reference|integer" />
        <attr name="btn_right_img" format="reference|integer" />
    </declare-styleable>

    <!-- 수조 관리 상태 View Custom View -->
    <declare-styleable name="TableAttrs">
        <attr name="table_limg" format="reference|integer" />
        <attr name="table_left_title" format="string"/>
        <attr name="table_left_contents" format="string"/>
        <attr name="table_rimg" format="reference|integer" />
        <attr name="table_right_title" format="string"/>
        <attr name="table_right_contents" format="string"/>
    </declare-styleable>

</resources>
