<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- 최상위 메뉴란 Custom View -->
    <declare-styleable name="TitleAttrs">
        <attr name="title_text" format="string" />
        <attr name="title_symbol" format="reference|integer" />
    </declare-styleable>

    <!-- 사용자 세부사항 기입란 Custom View -->
    <declare-styleable name="EditAttrs">
        <attr name="edit_text" format="string" />
        <attr name="edit_hint" format="string" />
    </declare-styleable>

    <!-- 하단 Button Custom View -->
    <declare-styleable name="BtnAttrs">
        <attr name="btn_left_text" format="string" />
        <attr name="btn_right_text" format="string" />
        <attr name="btn_left_img" format="reference|integer" />
        <attr name="btn_right_img" format="reference|integer" />
    </declare-styleable>
</resources>
